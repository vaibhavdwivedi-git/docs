                        },
                        "hasMore": {
                          "type": "boolean",
                          "description": "Whether more pages are available."
                        },
                        "statistics": {
                          "type": "object",
                          "properties": {
                            "totalActivities": {
                              "type": "integer",
                              "description": "Total number of activities."
                            },
                            "uniqueActions": {
                              "type": "integer",
                              "description": "Number of unique actions performed."
                            },
                            "uniqueStakers": {
                              "type": "integer",
                              "description": "Number of unique stakers."
                            },
                            "uniquePods": {
                              "type": "integer",
                              "description": "Number of unique pods."
