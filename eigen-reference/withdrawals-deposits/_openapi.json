                      "type": "object",
                      "properties": {
                        "errors": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "description": "Error message",
                          "example": ["Failed to fetch withdrawal"]
                        }
                      },
                      "required": ["errors"]
                    }
                  },
