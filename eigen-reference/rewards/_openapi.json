                          },
                          "token": {
                            "type": "string",
                            "description": "Address of the token",
                            "example": "0x1234567890abcdef1234567890abcdef12345678"
                          },
                          "claimedAmount": {
                            "type": "string",
                            "description": "Amount of tokens claimed",
                            "example": "1000000000000000000"
                          },
                          "claimedAt": {
                            "type": "number",
                            "description": "Timestamp of when the claim was made",
                            "example": 1727836560
                          }
