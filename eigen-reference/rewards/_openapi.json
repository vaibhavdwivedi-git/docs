                              },
                              "claimsCount": {
                                "type": "number",
                                "description": "Number of claims for this token",
                                "example": 50
                              }
                            },
                            "required": ["totalAmount", "claimsCount"]
                          },
                          "description": "Summary of claims grouped by token"
                        }
                      },
                      "required": [
                        "distinctRoots",
                        "distinctClaimers",
                        "distinctEarners",
                        "distinctRecipients",
                        "distinctTokens",
                        "totalClaimedAmount",
                        "firstClaimDate",
                        "lastClaimDate",
                        "tokensSummary"
                      ]
                    }
                  },
                  "required": ["data", "meta", "statistics"]
