                        "lastClaimDate": {
                          "type": "number",
                          "description": "Timestamp of the last claim",
                          "example": 1727836560,
                          "nullable": true
                        },
                        "tokensSummary": {
                          "type": "object",
                          "additionalProperties": {
                            "type": "object",
                            "properties": {
                              "totalAmount": {
                                "type": "string",
                                "description": "Total amount claimed for this token",
